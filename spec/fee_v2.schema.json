{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Fee Routing V2",
  "type": "object",
  "properties": {
    "selector": {"type": "integer", "minimum": 0, "maximum": 2},
    "fee": {"type": "integer", "minimum": 0, "maximum": 9223372036854775807},
    "fee_ct": {"type": "integer", "minimum": 0, "maximum": 9223372036854775807},
    "fee_it": {"type": "integer", "minimum": 0, "maximum": 9223372036854775807}
  },
  "required": ["selector", "fee", "fee_ct", "fee_it"],
  "additionalProperties": false,
  "examples": [
    {"selector": 2, "fee": 100, "fee_ct": 50, "fee_it": 50},
    {
      "selector": 3,
      "fee": 1,
      "fee_ct": 0,
      "fee_it": 1,
      "$comment": "invalid selector: must be <=2"
    }
  ],
  "$comment": "INV-FEE-01/02 see ECONOMICS.md#inv-fee-01/#inv-fee-02"
}
